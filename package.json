{
  "name": "taskseeder",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "start": "npm run build && node dist-server/index.js",
    "server:dev": "nodemon --exec \"npm run build:server && node dist-server/index.js\" --watch server --ext ts",
    "build:server": "tsc --project server/tsconfig.json",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e-ui": "playwright test --ui",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "alloy-lang": "^6.2.0-6",
    "papaparse": "^5.5.3",
    "tournament-organizer": "^3.9.1",
    "vue": "^3.5.13",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@playwright/test": "^1.52.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.15.30",
    "@types/papaparse": "^5.3.16",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^8.2.2",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-vue": "^10.2.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.10"
  }
}
