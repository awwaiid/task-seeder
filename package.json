{
  "name": "taskseeder",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "start": "npm run build && node dist-server/index.js",
    "server:dev": "nodemon --exec \"npm run build:server && node dist-server/index.js\" --watch server --ext ts",
    "build:server": "tsc --project server/tsconfig.json",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e-ui": "playwright test --ui",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "alloy-lang": "^6.2.0-17",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "papaparse": "^5.5.3",
    "sqlite3": "^5.1.6",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/node": "^22.19.3",
    "@types/papaparse": "^5.5.2",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "express": "^5.2.1",
    "jsdom": "^27.4.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "tournament-organizer": "^3.9.1",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.12"
  }
}
